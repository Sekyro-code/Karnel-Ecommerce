									<a href="{{path('app_cart')}}"><img src="{{asset('assets/icon/panier.png')}}" alt="logo_cart" class="logo_cart">
									</a>
									<a href="{{path('app_account')}}">Mon compte
										<small>({{app.user.firstname}})</small>
									</a>
									<a href="{{path('app_logout')}}">Deconnexion
